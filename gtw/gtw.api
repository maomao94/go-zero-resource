syntax = "v1"

import "doc/base.api"
import "doc/sys/user.api"
import "doc/message/message.api"
import "doc/resource/resource.api"

info(
	title: "网关服务"
	desc: "网关服务"
	author: "hehanpeng"
	email: "hehanpengyy@163.com"
	version: "v1"
)

@server(
	prefix: gtw/v1
	group: gtw
)
service gtw {
	@doc "ping"
	@handler ping
	get /ping returns (PingReply)
	
	@doc "pingHello"
	@handler pingHello
	get /pingHello returns (PingReply)
}